{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.17",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "7.1.18",
    "@types/react-router-dom": "5.1.8",
    "axios": "0.21.1",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "path": "0.12.7",
    "react": "^17.0.2",
    "react-content-loader": "6.0.3",
    "react-dom": "^17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "4.3.1",
    "redux": "4.1.0",
    "redux-localstorage-simple": "2.4.1",
    "redux-thunk": "2.3.0",
    "styled-components": "5.3.0",
    "styled-normalize": "8.0.7",
    "swiper": "6.8.4",
    "tslib": "2.3.1",
    "typescript": "4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run lint && react-scripts build",
    "build:production": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-heroku": "node server.js",
    "lint:js": "eslint --ignore-path .gitignore --ext .ts,.tsx,.js ./src/",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint './src/**/*.ts{,x}' --syntax 'css-in-js'",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "lint:fix": "npm-run-all lint:js:fix lint:css:fix",
    "prepare": "husky install",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:build": "docker-compose -f docker-compose.prod.yml build"
  },
  "engines": {
    "node": "14.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "4.14.172",
    "@types/react-select": "4.0.17",
    "@types/styled-components": "5.1.11",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.1",
    "jest-styled-components": "7.0.5",
    "lint-staged": "11.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "redux-immutable-state-invariant": "2.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "stylelint --fix --syntax 'css-in-js'",
      "prettier --write"
    ]
  }
}
